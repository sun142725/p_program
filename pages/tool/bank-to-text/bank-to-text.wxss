/* pages/tool/bank-to-text/bank-to-text.wxss */
.image-to-text{
  padding: 24rpx;
}
.preview{
  width: 100%;
  height: 400rpx;
  padding: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.preview .tempFilePath{
  width: 100%;
  height: 100%;
}
.preview .camera{
  width: 100%;
  height: 100%;
  border-radius: 4rpx;
  border: 1px dashed #666;
  display: flex;
  justify-content: center;
  align-items: center;
}
.preview .camera image{
  width: 200rpx;
  height:  150rpx;
}